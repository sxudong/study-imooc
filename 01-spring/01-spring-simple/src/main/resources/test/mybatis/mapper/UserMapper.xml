<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.SpringSourceAnalysis.ch09.mybatis.mapper.UserMapper">
  <insert id="insertUser" parameterType="com.SpringSourceAnalysis.ch09.mybatis.pojo.User">
    insert into user(name,age) values(#{name},#{age})
  </insert>

   <select id="getUser" resultType="com.SpringSourceAnalysis.ch09.mybatis.pojo.User" parameterType="java.lang.Integer">
     select * from user where id=#{id}
   </select>

</mapper>